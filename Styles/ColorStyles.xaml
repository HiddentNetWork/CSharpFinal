<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <LinearGradientBrush x:Key="cs_Background" StartPoint="0,0" EndPoint="0,5">
        <GradientStop Color="#292C35" Offset="0.0"/>
        <GradientStop Color="#FFBE76" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="cs_BorderBrush" Color="#CD6133"/>
    <SolidColorBrush x:Key="cs_ImgAct" Color="#FFA4B883"/>
    <SolidColorBrush x:Key="cs_imgPas" Color="#FFE6F6CD"/>
    <SolidColorBrush x:Key="cs_Txt" Color="#FFF5ECEB"/>
</ResourceDictionary>